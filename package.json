{
  "name": "nydus",
  "version": "0.3.2",
  "description": "WebSocket server using the nydus protocol, a simple RPC/PubSub protocol.",
  "main": "lib/index.js",
  "scripts": {
    "autotest": "mocha --watch -R spec --compilers js:babel/register --recursive test/",
    "build": "babel --out-dir lib/ *.js",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js ./",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "mocha -R spec --compilers js:babel/register --recursive test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus"
  },
  "keywords": [
    "nydus",
    "websockets",
    "websocket",
    "rpc",
    "pubsub"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus/issues"
  },
  "homepage": "https://github.com/tec27/nydus",
  "dependencies": {
    "backo": "^1.1.0",
    "cuid": "^1.2.5",
    "debug": "^2.2.0",
    "engine.io": "^1.5.2",
    "immutable": "^3.7.4",
    "nydus-protocol": "^0.3.1",
    "ruta3": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.7.4",
    "babel-eslint": "^4.0.5",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "engine.io-client": "^1.5.2",
    "eslint": "^1.6.0",
    "eslint-plugin-babel": "^2.1.1",
    "mocha": "^2.2.5",
    "nydus-client": "^0.3.0",
    "rimraf": "^2.4.3"
  }
