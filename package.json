{
  "name": "nydus",
  "version": "1.0.0",
  "description": "WebSocket server using the nydus protocol, a simple RPC/PubSub protocol.",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "autotest": "mocha --watch -R spec --compilers js:babel-register --recursive test/",
    "build": "babel --out-dir lib/ *.js",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js ./",
    "posttest": "yarn run lint",
    "prepublish": "yarn run lint && yarn run test && yarn run clean && yarn run build",
    "test": "mocha -R spec --compilers js:babel-register --recursive test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus"
  },
  "keywords": [
    "nydus",
    "websockets",
    "websocket",
    "rpc",
    "pubsub"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus/issues"
  },
  "homepage": "https://github.com/tec27/nydus",
  "dependencies": {
    "babel-preset-env": "^1.4.0",
    "backo": "^1.1.0",
    "cuid": "^1.2.5",
    "engine.io": "^3.1.0",
    "immutable": "^3.7.4",
    "nydus-protocol": "^1.0.0",
    "ruta3": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.2.3",
    "babel-register": "^6.3.13",
    "chai": "^3.2.0",
    "chai-as-promised": "^6.0.0",
    "engine.io-client": "^3.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "mocha": "^3.4.1",
    "nydus-client": "^0.3.0",
    "rimraf": "^2.4.3"
  }
}
